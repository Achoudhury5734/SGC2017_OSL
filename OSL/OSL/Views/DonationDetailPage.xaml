<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="OSL.Views.DonationDetailPage">
    <ContentPage.Content>
        <StackLayout>
            <Button Text="Complete Donation" Command="{Binding CompleteCommand}"/>
            <Button Text="Waste Donation" Command="{Binding WasteCommand}"/>
            <Button Text="Edit and Relist" Command="{Binding RelistCommand}"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>